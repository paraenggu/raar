            "type": "string"
          },
          {
            "name": "format",
            "in": "path",
            "description": "File extension of the audio encoding to get the audio file for.",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "successfull operation",
            "schema": {
              "type": "file"
            }
          }
